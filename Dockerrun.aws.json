{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client-react",
            "image": "taylorzhangyx/zhangyx.com-client-react",
            "hostname": "client-react",
            "essential": false,
            "memory": 256
        },
        {
            "name": "server-gin",
            "image": "taylorzhangyx/zhangyx.com-server-gin",
            "hostname": "server-gin",
            "essential": false,
            "memory": 256
        },
        {
            "name": "client-flask",
            "image": "taylorzhangyx/zhangyx.com-client-flask",
            "hostname": "client-flask",
            "essential": false,
            "memory": 256
        },
        {
            "name": "nginx",
            "image": "taylorzhangyx/zhangyx.com-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client-react",
                "client-flask",
                "server-gin"
            ],
            "memory": 128
        }
    ]
}
